  "scripts": {
    "start:desktop": "npx nx build client && npx nx build server && npx tsc -p apps/desktop/tsconfig.json && npx electron dist/apps/desktop/main.js",
    "build:desktop": "npx nx build client --configuration=production && npx nx build server --configuration=production && npx tsc -p apps/desktop/tsconfig.json && cp apps/desktop/package.json dist/apps/desktop/ && cd dist/apps/desktop && npm install --omit=dev && cd ../server && npm install --omit=dev && npm install tslib && cd ../../.. && npx prisma generate --schema=prisma/schema.prisma && cp -r node_modules/.prisma dist/apps/server/node_modules/",
    "package:desktop": "npm run build:desktop && npx electron-builder --config apps/desktop/electron-builder.json",
    "package:desktop:mac": "npm run build:desktop && npx electron-builder --config apps/desktop/electron-builder.json --mac",
    "package:desktop:win": "npm run build:desktop && npx electron-builder --config apps/desktop/electron-builder.json --win",
    "package:desktop:linux": "npm run build:desktop && npx electron-builder --config apps/desktop/electron-builder.json --linux"
  },
